            <Button fx:id="btnreg" layoutX="34.0" layoutY="200.0" mnemonicParsing="false" onMouseClicked="#register" prefHeight="26.0" prefWidth="182.0" style="-fx-background-color: #ff8826;" text="LET'S GO" textFill="#cbffc7">
