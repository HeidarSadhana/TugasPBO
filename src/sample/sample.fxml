                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
            <PasswordField fx:id="txtkpassreg" layoutX="34.0" layoutY="156.0" prefHeight="26.0" prefWidth="182.0" promptText="Konfirmasi Password">
